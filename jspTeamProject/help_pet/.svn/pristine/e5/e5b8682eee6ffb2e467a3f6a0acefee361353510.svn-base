/* 공지 */


.protect_notice_list {
    width: 100%;
    border-top: 2px solid green;
}

.protect_notice_tr {
    border-bottom: 1px solid #ccc;
}

.protect_notice_th,
.protect_notice_td {
    padding: 10px;
    font-size: 14px;
}

.protect_notice_td{
    text-align: center;
}

.protect_notice_list #tit {
    text-align: left;
}

.protect_notice_tr #tit:hover {
    text-decoration-color: chartreuse;
    text-decoration: underline ;
}
#protect_notice_btn{
    width: 90px;
    height: 25px;
    border: none;
    color: red;
    background-color: inherit;
    align-items: center;
}
