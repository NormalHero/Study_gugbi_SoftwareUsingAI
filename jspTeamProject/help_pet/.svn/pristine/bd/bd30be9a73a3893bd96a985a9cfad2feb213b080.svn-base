.info_area{
    position: relative;
    display: inline-block;
	border : 1px solid #ccc;
	border-left :none;
	border-right : none;
    background-color: #fafafa;
    padding: 33px 0;
    width: 100%
}
.info_inner{
	display : flex;
	justify-content:center;
    position: relative;
    width: 1200px;
    margin: 0 auto;
    z-index: 100;
}
.info_inner_ul{
    list-style: none;
    display: flex;
    justify-content: space-between;
    align-items: center;
}


.info_inner_li{
    list-style: none;
    margin: 0 auto;
    text-align: center;float: left;
    padding : 0 25px;
    
    
}


.footer_area{
	padding-left: 60px;
    display:flex;
    width: 100%;
    height: auto;    
    box-sizing: border-box;
}
li{
	list-style: none;
}
.footer_area_ul{
