<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http//mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Board">
	<insert id="question" parameterType="qnaVO">
		insert into tbl_ask (askName, askEmail, askTitle, askContent)
		values(#{userName},#{userEmail},#{userTitle},#{userContent})
	</insert>
</mapper>