			/* 페이징처리 css 시작*/
 .under_num {
    margin-top: 20px;
    text-align: center;
    font-size: 0;
}
.under_num a {
    display: inline-block;
    margin-left: 10px;
    padding: 5px 10px;
    border-radius: 100px;
    font-size: 12px;
}
.under_num a:first-child {
    margin-left: 0;
}

 .under_num a.bt {
    border: 1px solid #eee;
    background: #eee;
}

.under_num a.num {
    border: 1px solid #EEC0DB;
    font-weight: 600;
    color: #EEC0DB;
}

.under_num a.num.on {
    background: #EEC0DB;
    color: white;
}
/* 페이징처리 css종료*/
	
	
