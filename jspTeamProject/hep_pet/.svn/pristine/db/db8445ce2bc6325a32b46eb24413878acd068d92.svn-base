 /* 메인끝  */
.imgpet_wrap{
    text-align: center;
}


.mySlides_s{
  float: left;
  margin: 0 3px;
  margin-bottom: 20px;
}
.mySlides_ss{
  position: absolute;
  width: 400px;
  height: 300px;
  font-size: 20px;
  
  color: #bbb;
  background-color: rgba(0, 0, 0, 0.6);
}
.mySlides_t{
	top:20%;
